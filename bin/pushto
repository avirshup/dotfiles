#!/bin/bash

imagename=$1
current_registry=$2
new_registry=$3

if [ -z "${new_registry}" ] ; then
    echo 'usage: pushto [imagename] [current-registry] [new registry]'
    exit 1
fi



target="${new_registry}/${imagename}"

if [ "${current_registry}" != "-" ]; then
    source="${current_registry}/${imagename}"
    docker pull "${source}"
else
    source="${imagename}"
fi

docker tag "${source}" "${target}"
docker push "${target}"

    
