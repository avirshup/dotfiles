{
    "title": "cmd + Function Keys to Media Control",
    "rules": [
      {
        "description": "option + f10-12 for mute/vol down/vol up",
        "manipulators": [
          {
            "type": "basic",
            "from": {
              "key_code": "f10",
              "modifiers": {
                "mandatory": [
                  "option"
                ]
              }
            },
            "to": [
              {
                "key_code": "mute"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "f11",
              "modifiers": {
                "mandatory": [
                  "option"
                ]
              }
            },
            "to": [
              {
                "key_code": "volume_decrement"
              }
            ]
          },
          {
            "type": "basic",
            "from": {
              "key_code": "f12",
              "modifiers": {
                "mandatory": [
                  "option"
                ]
              }
            },
            "to": [
              {
                "key_code": "volume_increment"
              }
            ]
          }
        ]
      }
    ]
  }
  