{
    "title": "double-command application switchers",
    "rules": [
        {
            "description": "t + left-cmd + right-cmd -> Terminal.app",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "t",
                        "modifiers": {
                            "mandatory": ["left_gui", "right_gui"]
                        }
                    },
                    "to": [{"shell_command": "open -a 'Terminal.app'"}]
                }
            ]
        },
        {
            "description": "p + left-cmd + right-cmd -> pycharm",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "p",
                        "modifiers": {
                            "mandatory": ["left_gui", "right_gui"]
                        }
                    },
                    "to": [{"shell_command": "open -b com.jetbrains.pycharm"}]
                }
            ]
        },
        {
            "description": "f + left-cmd + right-cmd -> firefox",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f",
                        "modifiers": {
                            "mandatory": ["left_gui", "right_gui"]
                        }
                    },
                    "to": [{"shell_command": "open -a Firefox.app"}]
                }
            ]
        },
        {
            "description": "c + left-cmd + right-cmd -> teams",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "c",
                        "modifiers": {
                            "mandatory": ["left_gui", "right_gui"]
                        }
                    },
                    "to": [{"shell_command": "open -a 'Microsoft Teams.app'"}]
                }
            ]
        },
        {
            "description": "e + left-cmd + right-cmd -> mail.app",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": ["left_gui", "right_gui"]
                        }
                    },
                    "to": [{"shell_command": "open -a Mail.app"}]
                }
            ]
        }
    ]
}